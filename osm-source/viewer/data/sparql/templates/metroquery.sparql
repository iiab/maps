SELECT ?place ?placeLabel ?placeDescription ?location ?image ?marker_size ?marker_color ?type ?marker_symbol ?lat ?long WHERE {
  ?place wdt:P131 wd:Q1353;
    wdt:P31 wd:Q928830;
    wdt:P1373 ?num.
  FILTER(?num > 10000 )
  ?place wdt:P625 ?location.
  OPTIONAL { ?place wdt:P18 ?image. }
  BIND(STRBEFORE(STRAFTER(STR(?location), " "), ")") AS ?lat)
  BIND(STRBEFORE(STRAFTER(STR(?location), "Point("), " ") AS ?long)
  SERVICE wikibase:label { bd:serviceParam wikibase:language "en". }
}